@{
  Layout = "_Layout";
}

@using OrderTracker.Models;
<h1>Here is a list of Vendors</h1>
@if(@Model.Count == 0)
{
  <p>There are no vendors in the list.</p>
}
@foreach (Vendor entry in Model)
{
  <h5><a href="/vendors/@entry.Id">@entry.Name</a></h5>
}
<p><a href="/vendors/new">Add a new vendor</a></p>
<p><a href='/'>Back to home page</a></p>


